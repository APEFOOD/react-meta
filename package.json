{
  "name": "reactive-meta",
  "version": "0.1.3",
  "description": "React library for dynamically appending meta (and other) tags to the document head",
  "main": "dist/lib/index.js",
  "scripts": {
    "start": "./node_modules/.bin/parcel src/docs/index.html",
    "build": "./node_modules/.bin/webpack --mode=production", 
    "build:docs": "./node_modules/.bin/parcel build src/docs/index.js -d ./dist/docs && cp src/docs/index.html dist/docs/index.html"
  },
  "author": {
    "name": "Kabelo", 
    "email": "kabelonkakala20@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "babel-core": "6",
    "babel-loader": "^8.0.0-beta",
    "parcel-bundler": "^1.8.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-node-externals": "^1.7.2"
  }
}
